{
  "name": "sabeeo-api",
  "version": "1.0.0",
  "description": "API para Sabeeo",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"tsc -w \" \"nodemon src/app.ts\"",
    "clean": "concurrently \"rimraf dist\" \"rimraf tsconfig.tsbuildinfo\" \"tsc\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:create": "npm run typeorm -- migration:create",
    "migrations:up": "npm run typeorm -- migration:run -d ./src/configs/database/datasource.config.ts",
    "migrations:show": "npm run typeorm -- migration:show -d ./src/configs/database/datasource.config.ts",
    "migrations:drop": "npm run typeorm -- migration:drop",
    "migrations:rollback": "npm run typeorm -- migration:revert -d ./src/configs/database/datasource.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/sabeeo1/sabeeo-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/sabeeo1/sabeeo-api/issues"
  },
  "homepage": "https://gitlab.com/sabeeo1/sabeeo-api#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bullmq": "^3.6.5",
    "class-transformer": "^0.5.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase": "^9.17.1",
    "firebase-admin": "^11.5.0",
    "helmet": "^6.0.1",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.0",
    "nodemailer-express-handlebars": "^6.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/i18n": "^0.13.6",
    "@types/node": "^18.13.0",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/passport-jwt": "^3.0.8",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.20",
    "tsconfig-paths": "^4.1.2"
  }
}
